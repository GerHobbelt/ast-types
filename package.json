{
  "author": "Ben Newman <bn@cs.stanford.edu>",
  "name": "@gerhobbelt/ast-types",
  "version": "0.13.2-30",
  "description": "Esprima-compatible implementation of the Mozilla JS Parser API",
  "keywords": [
    "ast",
    "abstract syntax tree",
    "hierarchy",
    "mozilla",
    "spidermonkey",
    "parser api",
    "esprima",
    "types",
    "type system",
    "type checking",
    "dynamic types",
    "parsing",
    "transformation",
    "syntax"
  ],
  "homepage": "http://github.com/benjamn/ast-types",
  "repository": {
    "type": "git",
    "url": "git://github.com/GerHobbelt/ast-types.git"
  },
  "license": "MIT",
  "main": "main.js",
  "types": "main.d.ts",
  "files": [
    "*.ts",
    "*.js",
    "*.json",
    "def/**/*.[js,ts]",
    "gen/**/*.[js,ts]",
    "lib/**/*.[js,ts]",
    "types/**/*.[js,ts]"
  ],
  "scripts": {
    "gen": "ts-node --transpile-only script/gen-types.ts",
    "tsc": "tsc --noEmit",
    "compile-tests": "cd test && tsc",
    "clean-test-data": "bash -c test/clean.sh",
    "test": "npm run gen && npm run build && npm run mocha",
    "mocha": "bash -c test/run.sh",
    "clean": "ts-emit-clean && npm run clean-test-data",
    "build": "tsc && ts-add-module-exports && npm run compile-tests",
    "orig-prepack": "npm run clean && npm run gen && npm run build",
    "postpack": "npm run clean",
    "pub": "npm publish --access public"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/parser": "7.5.5",
    "@babel/types": "7.5.5",
    "@types/esprima": "4.0.2",
    "@types/glob": "7.1.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.8",
    "espree": "6.0.0",
    "esprima": "4.0.1",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "flow-parser": "0.103.0",
    "glob": "7.1.4",
    "mocha": "6.2.0",
    "recast": "0.18.1",
    "reify": "0.20.12",
    "ts-add-module-exports": "1.0.0",
    "ts-emit-clean": "1.0.0",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">= 4.0"
  }
}
