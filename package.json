{
  "author": "Ben Newman <bn@cs.stanford.edu>",
  "name": "@gerhobbelt/ast-types",
  "version": "0.12.1-27",
  "description": "Esprima-compatible implementation of the Mozilla JS Parser API",
  "keywords": [
    "ast",
    "abstract syntax tree",
    "hierarchy",
    "mozilla",
    "spidermonkey",
    "parser api",
    "esprima",
    "types",
    "type system",
    "type checking",
    "dynamic types",
    "parsing",
    "transformation",
    "syntax"
  ],
  "homepage": "http://github.com/benjamn/ast-types",
  "repository": {
    "type": "git",
    "url": "git://github.com/GerHobbelt/ast-types.git"
  },
  "license": "MIT",
  "main": "main.js",
  "types": "main.d.ts",
  "scripts": {
    "gen": "ts-node --transpile-only script/gen-types.ts",
    "tsc": "tsc --noEmit",
    "compile-tests": "cd test && tsc",
    "test": "npm run gen && npm run tsc && npm run compile-tests && npm run mocha",
    "mocha": "bash -c test/run.sh",
    "clean": "ts-emit-clean",
    "build": "tsc && ts-add-module-exports && npm run compile-tests",
    "prepack": "npm run clean && npm run gen && npm run build",
    "postpack": "npm run clean",
    "pub": "npm publish --access public"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/parser": "7.3.1",
    "@babel/types": "7.3.0",
    "@types/esprima": "4.0.2",
    "@types/glob": "7.1.1",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.18",
    "espree": "5.0.0",
    "esprima": "4.0.1",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "flow-parser": "0.91.0",
    "glob": "7.1.3",
    "mocha": "5.2.0",
    "recast": "0.16.2",
    "reify": "0.18.1",
    "ts-add-module-exports": "1.0.0",
    "ts-emit-clean": "1.0.0",
    "ts-node": "8.0.2",
    "typescript": "3.2.4"
  },
  "engines": {
    "node": ">= 4.0"
  }
}
